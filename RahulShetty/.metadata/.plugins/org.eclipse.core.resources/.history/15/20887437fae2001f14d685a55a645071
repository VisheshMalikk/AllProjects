package StringPrograms;

public class S1 {

	public static void main(String[] args) {
		
		String s1 = new String("Vishesh");
		s1.concat(" Malik");
		
		System.out.println("Using String class :" + s1);
		
		System.out.println("------------------------------------");
		
		StringBuilder sb = new StringBuilder("Vishesh");
		sb.append(" Malik");
		
		System.out.println("Using String Builder :" + sb);
		
	}

}
