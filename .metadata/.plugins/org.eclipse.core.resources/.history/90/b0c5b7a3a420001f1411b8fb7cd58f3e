package ArrayPrograms;

public class sum {

	public static void main(String[] args) {

		sum obj = new sum();
		int resultAddition = obj.addition(10, 5);
		int resultSub= obj.substraction(10, 2);
		int resultdiv =obj.division(4,2);
		int resultmult =obj.multiplication(1489754,567589);
		System.out.println("Division"  +  resultdiv );
		System.out.println("Addition : " + resultAddition);
		System.out.println("Subtraction " + resultSub);
	    System.out.println("Multiplication:"  + resultmult);
	}
	
	
	
	
	
	
	

	public int addition(int a, int b) {
		int sum = a + b;
		return sum;
	}

	public int substraction(int x, int y) {
		int sub = x - y;
		return sub;
	}

	public int division(int c, int d) {
		int div = c / d;
	return div;
	}

	public int multiplication(int k, int l) {
		int mult = k * l;
	return mult;
			}

}
