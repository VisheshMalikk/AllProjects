package ArrayPrograms;

public class CreateTwoArrayFromOneOddEven {

	public static void main(String[] args) {
		
		int[] a = {3,5,1,9,6,2};
		
		int[] b = new int[3];
		int[] c = new int[3];
		
		for (int i = 0; i < a.length; i++) {
			if(a[i]%2 == 0) {
				b[i] = a[i];
			} else {
				c[i] = a[i];
			}
		}
		System.out.println("Array b : " + b);
		System.out.println("Array c : " + c);
	}

}
