package GetRequest;

import org.testng.annotations.Test;

import io.restassured.RestAssured;

public class createGetRequest {
	
	
	@Test
	public void getBooking() {
		
		RestAssured.given()
		.log().all()
		.baseUri("https://restful-booker.herokuapp.com/")
		.basePath("booking/{id}")
		.pathParam("id", 2)
		.when()
		.get()
		.then()
		.log().all().assertThat().statusCode(200);	
	}

}
